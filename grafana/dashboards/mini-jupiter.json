{
  "id": null,
  "uid": "mini-jupiter",
  "title": "Mini-Jupiter Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests / sec",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(rate(mini_jupiter_http_requests_total[1m]))",
          "legendFormat": "rps",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency (P95/P99)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(mini_jupiter_http_request_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(mini_jupiter_http_request_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p99",
          "refId": "B"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "In-flight Requests",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(mini_jupiter_http_inflight_requests)",
          "legendFormat": "inflight",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Errors by Code (rate)",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(rate(mini_jupiter_http_error_total[1m])) by (code)",
          "legendFormat": "{{code}}",
          "refId": "A"
        }
      ]
    }
  ]
}
